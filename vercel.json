{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
      "config": {
        "installCommand": "pip install -r requirements.txt && python download_models.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  }
}
